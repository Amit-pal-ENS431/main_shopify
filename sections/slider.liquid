<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css">
<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

<div class="wrapper page-width">
  <div class="main-img">
    <h1 class="textes_color 1">{{ section.settings['image-first'] }}</h1>
  </div>
  <div class="second-img">
    {% for i in section.blocks %}
    <div class="inner">
     
      <div class="slider_textes">
         <div class="slider_image_slick">
        <img src="{{ i.settings.first-img | img_url: 'original' }}" alt="" width="100%" height="auto" loading="lazy">
      </div>
        <h2 class="textes_color 2">{{ i.settings.person_name }}</h2>
        <h3 class="textes_color 3">{{ i.settings.person_designation }}</h3>
        <p class="textes_color 4">{{ i.settings.person_description }}</p>
      </div>
    </div>
    {% endfor %}
  </div>

<div>
  <div class="for_matress_image_text_heading">
     <h2 class="textes_color 5">{{ section.settings['for_matress_image_text_heading'] }}</h2>
</div>
   <div class="for_matress_image_textes">
 <div class="for_matress_image_text_image">
        <img src="{{ section.settings.for_matress_image_text_for_image | img_url: 'original' }}" alt="" width="100%" height="auto" loading="lazy">
      </div>
     <div class="for_matress_all_textes_flex">
     <div class="for_matress_image_texte_all_textes">
       <div class="matress_number">
         <span class="textes_color">
           {{ section.settings.for_matress_image_text_for_number }}
       </span>
       </div>
      <div class="for-text_head_paragragh">
        <p class="textes_color 6">{{ section.settings.for_matress_image_text_for_heading }}</p>
        <p class="textes_color 7">{{ section.settings.for_matress_image_text_for_textes }}</p>
      </div>
       </div>
        <div class="for_matress_image_texte_all_textes">
       <div class="matress_number">
         <span class="textes_color 8">
           {{ section.settings.for_matress_image_text_for_number_1 }}
       </span>
       </div>
      <div class="for-text_head_paragragh">
        <p class="textes_color 9">{{ section.settings.for_matress_image_text_for_heading_1 }}</p>
        <p class="textes_color 10">{{ section.settings.for_matress_image_text_for_textes_1 }}</p>
      </div>
          </div>
           <div class="for_matress_image_texte_all_textes">
       <div class="matress_number">
         <span class="textes_color 11">
           {{ section.settings.for_matress_image_text_for_number_2 }}
       </span>
       </div>
      <div class="for-text_head_paragragh">
        <p class="textes_color 12">{{ section.settings.for_matress_image_text_for_heading_2 }}</p>
        <p class="textes_color 13">{{ section.settings.for_matress_image_text_for_textes_2 }}</p>
      </div>
             </div>
              <div class="for_matress_image_texte_all_textes">
       <div class="matress_number">
         <span class="textes_color 14">
           {{ section.settings.for_matress_image_text_for_number_3 }}
       </span>
       </div>
      <div class="for-text_head_paragragh">
        <p class="textes_color 15">{{ section.settings.for_matress_image_text_for_heading_3 }}</p>
        <p class="textes_color16">{{ section.settings.for_matress_image_text_for_textes_3 }}</p>
      </div>
</div>



</div>

