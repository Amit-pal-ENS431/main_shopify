  "blocks": [
    {
      "type": "slider",
      "name": "Slider Block",
      "settings": [
        {
          "type": "image_picker",
          "id": "first-img",
          "label": "Upload Image"
        },
        {
          "type": "text",
          "id": "person_name",
          "label": "Name"
        },
        {
          "type": "text",
          "id": "person_designation",
          "label": "Designation"
        },
        {
          "type": "textarea",
          "id": "person_description",
          "label": "Description"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "slider"
    }
  ]
}
