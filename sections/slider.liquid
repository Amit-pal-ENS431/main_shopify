<style>
 h1.textes_color.\31 {
    margin-bottom: 28px;
    margin-top: 25px;
}
  h2.textes_color.\35 {
      margin-bottom: 10rem;
    margin-top: 25px;
}
  .circle_presentation_for_flex{
    display:flex;
    padding:5px;
    justify-content:center;
    align-items:center;
  }
  .circle_presentation{
    list-style-type: none;
    height:20px;
    width:20px;
    border: 5px solid white;
    border-radius:50%;
  }
  .textes_color{
    color:white;
    padding:0px;
    margin:2px;
  }
  .main-img{
    display:flex;
    justify-content:center;
    align-item:center;
    
  }
  .wrapper{
    background:#111111;
    padding:0;
  }
  .slider_textes {
    text-align: center;
    background: #2e2f3c;
    margin-right: 15px;
    padding-top:100px;
    border-radius:30px;
    position: relative;
    min-height: 350px;
    padding-left: 20px;
    padding-right: 20px;
}
  .slider_image_slick{
    position:absolute;
    width: 50%;
    bottom: 75%;
    left: 25%;
  }
.slider_image_slick img {
    border-radius: 50%;
}
  .inner{
     margin-top: 15rem;
     margin-bottom: 6rem;
  }
  .for_matress_image_text_heading{
    display:flex;
    align-items:center;
    justify-content:center;
  }
  .for_matress_image_textes{
    display:flex;
    width:100%;
  }
  .for_matress_image_text_image{
    width:140%;
  
  }
  .for_matress_image_text_image img{
    width:100%;
      height:85%
  }
  .for_matress_all_textes_flex{
    display:flex;
    flex-direction:column;
    width: 117%;
  }
  .for_matress_image_texte_all_textes{
    display:flex;
   
    padding:10px;
  }
  .matress_number span{
    margin:15px;
    display: flex;
    justify-content: center;
    align-items:center;
    height: 23px;
    min-width: 23px;
    border-radius: 50%;
    border: solid #e5e7eb;
    box-sizing: border-box;
  }
  .slick-dots li button:before{
    border: 5px solid white;
    border-radius: 50%;
    font-size: 0px;
    position:absolute;
    top:0%;
  }
  .slick-dots{
        bottom: 100%;
     
  }
</style>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

<script>
  $(document).ready(function() {
    $('.second-img').slick({
      speed: 1500,
      slidesToShow: 3,
      slidesToScroll: 1,
      autoplay: false,
      arrows: true,
      dots: true,
      autoplaySpeed: 2000,
      responsive: [
        {
          breakpoint: 1024,
          settings: {
            slidesToShow: 3,
            slidesToScroll: 3,
            infinite: true,
            arrows: true,
            dots: false
          }
        },
        {
          breakpoint: 600,
          settings: {
            slidesToShow: 2,
            slidesToScroll: 2
          }
        },
        {
          breakpoint: 480,
          settings: {
            slidesToShow: 2,
            slidesToScroll: 1
          }
        }
      ]
    });
  });
</script>
